#include "MierzenieCzasu.h"
#include <iostream>
#include <chrono>

using namespace std;
using namespace std::chrono;

void MierzenieCzasu::start() {
    poczatek = high_resolution_clock::now();
}

void MierzenieCzasu::stop() {
    koniec = high_resolution_clock::now();
}

long MierzenieCzasu::ileZajelo() {

    return duration_cast<nanoseconds>(Czas::czasKoncowy - Czas::czasPoczatkowy).count();

}
